source 'http://10.0.0.192/git/TP-Pod-Specs.git'
source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '9.0'
inhibit_all_warnings!

def shared_pods
    pod 'Fabric'
    pod 'Crashlytics'
    pod 'MBProgressHUD'
    pod 'AFNetworking', '~> 3.0'
    pod 'ActionSheetPicker-3.0'
end

target 'BitPoint' do
    shared_pods
end
